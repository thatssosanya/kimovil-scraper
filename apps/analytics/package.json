{
  "name": "@repo/analytics",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "build": "npm run build:analytics && tsc",
    "build:analytics": "esbuild public/analytics.js --bundle --minify --drop:console --outfile=public/analytics.min.js",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "migrate": "tsx scripts/migrate.ts",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@clickhouse/client": "^1.8.1",
    "@effect/platform": "^0.94.0",
    "@effect/schema": "^0.75.5",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/node": "^1.4.0",
    "@sinclair/typebox": "^0.34.45",
    "dotenv": "^17.2.3",
    "effect": "^3.18.4",
    "elysia": "^1.4.0",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@effect/vitest": "^0.27.0",
    "@types/node": "^22.10.2",
    "esbuild": "^0.25.0",
    "tsx": "^4.19.2",
    "vitest": "^2.1.8"
  }
}
